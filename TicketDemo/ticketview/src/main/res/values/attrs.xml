        <attr name="line_drawWeight" format="float" />
        <attr name="line_color" format="color" />
        <attr name="line_padding" format="dimension" />
        <attr name="line_width" format="dimension" />
        <attr name="line_gap" format="dimension" />
    </declare-styleable>
</resources>